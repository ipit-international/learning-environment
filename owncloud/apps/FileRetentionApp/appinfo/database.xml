<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
    <name>*dbname*</name>
    <create>true</create>
    <overwrite>false</overwrite>
    <charset>utf8</charset>
    <table>
    	<name>*dbprefix*files</name>
    	<declaration>
    		<field>
    			<name>id</name>
    			<type>integer</type>
    			<default>0</default>
    			<notnull>true</notnull>
    			<autoincrement>1</autoincrement>
    		</field>
    		<field>
    			<name>delete_time</name>
    			<type>text</type>
    			<length>64</length>
    			<notnull>true</notnull>
    		</field>
    		<field>
    			<name>path</name>
    			<type>text</type>
    			<length>4096</length>
    			<notnull>true</notnull>
    		</field>
    		<field>
    		        <name>actual_deleted_time</name>
    		        <type>text</type>
    		        <length>64</length>
    		        <notnull>false</notnull>
    		</field>
    	</declaration>
    </table>
</database>
